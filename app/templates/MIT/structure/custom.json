{
  "type": "Div",
  "style": ".container",
  "children": [
    {
      "type": "Text",
      "style": ".title",
      "bind": "data.title"
    },
    {
      "type": "Div",
      "style": ".dateRow",
      "children": [
        {
          "type": "Text",
          "style": ".date",
          "bind": "data.startDate"
        },
        {
          "type": "Raw",
          "style": ".date",
          "bind": "-",
          "if": "data.startDate && data.endDate"
        },
        {
          "type": "Text",
          "style": ".date",
          "bind": "data.endDate"
        }
      ]
    },
    {
      "type": "Text",
      "style": ".subtitle",
      "bind": "data.subtitle"
    },
    {
      "type": "Text",
      "style": ".location",
      "bind": "data.location"
    },
    {
      "type": "Html",
      "style": ".description",
      "bind": "data.description"
    }
  ]
}